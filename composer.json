{
    "name": "eventpad/html-to-pdf-bundle",
    "type": "symfony-bundle",
    "description": "Convert HTML to PDF in Symfony",
    "require": {
        "php": ">=5.4",

        "mikehaertl/phpwkhtmltopdf": "1.2.*",
        "h4cc/wkhtmltopdf-amd64": "*",
        "h4cc/wkhtmltopdf-i386": "*",
        "symfony/process": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mikey179/vfsStream": "1.2.*"
    },
    "authors": [
        {
            "name": "Adam Quaile",
            "email": "adamquaile@gmail.com"
        }
    ],
    "keywords": ["pdf", "pdf-to-html"],
    "autoload": {
        "psr-4": {
            "EP\\Bundle\\HtmlToPdfBundle\\": ["", "tests/"]
        }
    }
}
